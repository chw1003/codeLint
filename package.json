{
  "name": "newcodelint",
  "version": "1.0.0",
  "description": "codelint新测试项目",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@git.xchanger.cn:liwei/newcodelint.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "less": "^3.11.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "^4.41.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.7",
    "pre-commit": "^1.2.2",
    "prettier": "1.19.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^3.0.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
